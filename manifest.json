{
  "manifest_version": 3,
  "name": "NettoyantAda",
  "version": "1.1",

  "description": "Nettoyant Ada, pour masquer les spammeurs",
  
  "icons": {
    "48": "icons/ada-48.png",
    "96": "icons/ada-96.png"
    },
"permissions": [
  "storage"
],
  "content_scripts": [
    {
      "matches": ["https://www.atelierdesauteurs.com/*"],
      "js": ["dontwanna.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
